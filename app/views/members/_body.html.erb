

<table class="attr">
  <tr>
    <th width="150">Number</th>
    <td><%= @member.number %></td>
  </tr>
  <tr>
    <th>Username</th>
    <td><%= @member.name %></td>
  </tr>
  <tr>
    <th>Name</th>
    <td><%= @member.full_name %></td>
  </tr>
  <tr>
    <th>Sex</th>
    <td><%= @member.sex == 1? "Man" : "Woman"%></td>
  </tr>
  <tr>
    <th>Birthday</th>
    <td><%= @member.birthday&.strftime("%Y/%m/%d") %></td>
  </tr>
  <tr>
    <th>Email</th>
    <td><%= @member.email %></td>
  </tr>
  <tr>
    <th>Admin</th>
    <td><%= @member.administrator?? "〇": "＿" %></td>
  </tr>
</table>